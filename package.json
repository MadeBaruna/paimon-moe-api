    "fastify": "^3.14.0",
    "fastify-cors": "^5.2.0",
    "fastify-plugin": "^3.0.0",
    "firebase-admin": "^9.6.0",
    "nano-memoize": "^1.2.1",
    "node-fetch": "^2.6.1",
    "node-schedule": "^2.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.31",
    "xxhash": "^0.3.0"
